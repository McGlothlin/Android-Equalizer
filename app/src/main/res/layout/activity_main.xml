<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/resetButton"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginEnd="280dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/reset"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/enabledSwitch"
        android:layout_width="101dp"
        android:layout_height="15dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/enabled"
        app:layout_constraintBottom_toBottomOf="@+id/resetButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/resetButton" />

    <SeekBar
        android:id="@+id/bassBoostSeekBar"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:max="100"
        android:progress="0"
        app:layout_constraintBottom_toTopOf="@+id/bassBoostLabel"
        app:layout_constraintEnd_toEndOf="@+id/enabledSwitch"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/resetButton" />

    <TextView
        android:id="@+id/bassBoostLabel"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginBottom="32dp"
        android:text="@string/bass_boost_label"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/bassBoostSeekBar"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bassBoostSeekBar" />

    <android.widget.VerticalSeekBar
        android:id="@+id/verticalSeekBar1"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="20dp"
        android:layout_height="300dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/verticalSeekBar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <android.widget.VerticalSeekBar
        android:id="@+id/verticalSeekBar2"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="20dp"
        android:layout_height="300dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/verticalSeekBar3"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBar1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <android.widget.VerticalSeekBar
        android:id="@+id/verticalSeekBar3"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="20dp"
        android:layout_height="300dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/verticalSeekBar4"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBar2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <android.widget.VerticalSeekBar
        android:id="@+id/verticalSeekBar4"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="20dp"
        android:layout_height="300dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/verticalSeekBar5"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBar3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <android.widget.VerticalSeekBar
        android:id="@+id/verticalSeekBar5"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="20dp"
        android:layout_height="300dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/verticalSeekBar6"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBar4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <android.widget.VerticalSeekBar
        android:id="@+id/verticalSeekBar6"
        style="@android:style/Widget.DeviceDefault.SeekBar"
        android:layout_width="20dp"
        android:layout_height="300dp"
        android:max="100"
        android:progress="50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/verticalSeekBar5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />
</android.support.constraint.ConstraintLayout>